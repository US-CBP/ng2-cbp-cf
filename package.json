    "author": "CBP Common Framework",
    "license": "CC0-1.0",
    "engines": {
        "node": ">=6.4"
    },
    "main": "index.ts",
    "module": "index.ts",
    "typings": "typings.d.ts",
    "scripts": {
        "prebuild": "npm run-s clean && npm run-s lint && npm run-s test",
        "build": "cross-env NODE_ENV=production webpack --display-chunks",
        "tsc": "tsc --project \"tsconfig-declaration.json\" && npm run-s copy-d-ts",
        "copy-d-ts": "cpx dts/**/*d.ts dist",
        "watch": "npm run-s clean && cross-env NODE_ENV=development webpack --watch",
        "nowatch": "npm run-s clean && cross-env NODE_ENV=development webpack",
        "clean": "del-cli dist && del-cli dts",
        "lint": "npm run-s lint:js && npm run-s lint:ts && npm run-s lint:css",
        "lint:js": "eslint -c .eslintrc.yml *.js",
        "lint:ts": "tslint src/**/*.ts",
        "lint:css": "stylelint \"theme/*.scss\" \"src/**/*.scss\" --config stylelint-config.json",
        "test": "karma start --single-run",
        "test:watch": "karma start",
        "prepublish": "npm run-s build",
        "semantic-release": "semantic-release pre && npm publish && semantic-release post"
    },
    "dependencies": {
        "@angular/animations": "^4.1.0",
        "@angular/common": "^4.1.0",
        "@angular/compiler": "^4.1.0",
        "@angular/core": "^4.1.0",
        "@angular/flex-layout": "2.0.0-beta.8",
        "@angular/forms": "^4.1.0",
        "@angular/http": "^4.1.0",
        "@angular/material": "2.0.0-beta.3",
        "@angular/platform-browser": "^4.1.0",
        "@angular/platform-browser-dynamic": "^4.1.0",
        "@angular/router": "^4.1.0",
        "cbp-theme": "~0.4.0",
        "core-js": "^2.4.1",
        "hammerjs": "~2.0.8",
        "lodash": "^4.17.4",
        "material-design-icons": "^3.0.1",
        "moment": "~2.17.1",
        "reflect-metadata": "~0.1.9",
        "rxjs": "^5.3.0",
        "zone.js": "^0.8.9"
    },
    "devDependencies": {
        "@types/core-js": "~0.9.35",
        "@types/hammerjs": "~2.0.8",
        "@types/jasmine": "2.5.45",
        "@types/lodash": "4.14.50",
        "@types/node": "~6.0.41",
        "angular2-template-loader": "0.6.0",
        "autoprefixer": "~6.7.6",
        "awesome-typescript-loader": "~3.0.3",
        "babel-core": "~6.22.1",
        "babel-loader": "~6.2.10",
        "body-parser": "~1.16.0",
        "codelyzer": "~2.0.0-beta.4",
        "cpx": "~1.5.0",
        "cross-env": "~3.1.4",
        "css-loader": "~0.26.1",
        "del-cli": "~0.2.1",
        "eslint": "~3.15.0",
        "extract-text-webpack-plugin": "2.0.0-rc.3",
        "file-loader": "~0.10.0",
        "html-loader": "~0.4.5",
        "html-webpack-plugin": "~2.28.0",
        "image-webpack-loader": "~3.2.0",
        "jasmine-core": "~2.5.2",
        "karma": "~1.4.1",
        "karma-chrome-launcher": "~2.0.0",
        "karma-coverage": "~1.1.1",
        "karma-jasmine": "~1.1.0",
        "karma-mocha-reporter": "^2.2.2",
        "karma-sourcemap-loader": "~0.3.7",
        "karma-webpack": "~2.0.2",
        "minx": "r-park/minx.git",
        "morgan": "~1.8.0",
        "node-sass": "~4.5.0",
        "node-uuid": "~1.4.7",
        "nodemon": "~1.11.0",
        "npm-run-all": "~4.0.1",
        "raw-loader": "~0.5.1",
        "sass-loader": "~4.1.1",
        "semantic-release": "~6.3.2",
        "source-map-loader": "~0.1.6",
        "style-loader": "~0.13.1",
        "stylelint": "^7.10.1",
        "stylelint-order": "^0.4.4",
        "systemjs": "~0.19.39",
        "systemjs-builder": "~0.15.32",
        "ts-helpers": "~1.1.2",
        "tslint": "~4.4.2",
        "typescript": "~2.1.5",
        "url-loader": "~0.5.7",
        "webpack": "^2.2.1",
        "webpack-dev-server": "~2.4.1",
        "webpack-shell-plugin": "~0.5.0",
        "winston": "~2.3.1"
    },
    "keywords": [
