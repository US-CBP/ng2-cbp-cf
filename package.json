    "description": "CBP Common Framework for Angular2",
    "homepage": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/US-CBP/ng2-cbp-cf.git"
    },
    "contributors": [
        {
            "name": "Xavier Mouligneau",
            "email": "xmouligneau@gmail.com"
        },
