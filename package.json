{
    "name": "ng2-cbp-cf",
    "version": "0.0.1",
    "description": "CBP Common Framework for Angular2",
    "homepage": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/US-CBP/ng2-cbp-cf.git"
    },
    "contributors": [
        {
          "name": "Xavier Mouligneau",
          "email": "xmouligneau@gmail.com"
        },
        {
          "name": "Martin Francisco",
          "email": "martin2great@yahoo.com"
        }
    ],
    "author": "CBP Common Framework",
    "license": "CC0-1.0",
    "engines": {
        "node": ">=6.4"
    },
    "main": "dist/ng2-cbp-cf.js",
    "module": "ng2-cbp-cf.js",
    "typings": "dist/ng2-cbp-cf.d.ts",
    "scripts": {
        "prebuild": "npm run-s clean && npm run-s lint && npm run-s test",
        "build": "cross-env NODE_ENV=production webpack --display-chunks",
        "tsc": "tsc --project \"tsconfig-declaration.json\" && npm run-s copy-d-ts",
        "copy-d-ts": "cpx dts/**/*d.ts dist",
        "watch": "cross-env NODE_ENV=development webpack --watch",
        "nowatch": "npm run-s clean && cross-env NODE_ENV=development webpack",
        "clean": "del-cli dist && del-cli dts",
        "lint": "npm run-s lint:js && npm run-s lint:ts",
        "lint:js": "eslint -c .eslintrc.yml *.js",
        "lint:ts": "tslint src/**/*.ts",
        "test": "karma start --single-run",
        "test:watch": "karma start",
        "prepublish": "npm run-s build",
        "semantic-release": "semantic-release pre && npm publish && semantic-release post"
    },
    "dependencies": {
        "@angular/common": "~2.1.0",
        "@angular/compiler": "~2.1.0",
        "@angular/core": "~2.1.0",
        "@angular/forms": "~2.1.0",
        "@angular/http": "~2.1.0",
        "@angular/platform-browser": "~2.1.0",
        "@angular/platform-browser-dynamic": "~2.1.0",
        "@angular/router": "~3.1.0",
        "core-js": "~2.4.1",
        "cbp-theme": "~0.4.0",
        "lodash": "^4.16.4",
        "moment": "~2.15.1",
        "reflect-metadata": "~0.1.8",
        "rxjs": "~5.0.0-rc.1",
        "zone.js": "~0.6.25"
    },
    "devDependencies": {
        "@types/lodash": "~4.14.37",
        "@types/jasmine": "~2.2.34",
        "@types/node": "~6.0.41",
        "angular2-template-loader": "~0.5.0",
        "autoprefixer": "~6.5.0",
        "awesome-typescript-loader": "~2.2.4",
        "babel-core": "~6.16.0",
        "babel-loader": "~6.2.5",
        "body-parser": "~1.15.2",
        "codelyzer": "1.0.0-beta.0",
        "cpx": "~1.5.0",
        "cross-env": "~3.0.0",
        "css-loader": "~0.25.0",
        "del-cli": "~0.2.0",
        "eslint": "~3.6.1",
        "extract-text-webpack-plugin": "2.0.0-beta.4",
        "file-loader": "~0.9.0",
        "html-loader": "~0.4.4",
        "html-webpack-plugin": "~2.22.0",
        "image-webpack-loader": "~2.0.0",
        "jasmine-core": "~2.5.2",
        "karma": "~1.3.0",
        "karma-chrome-launcher": "~2.0.0",
        "karma-coverage": "~1.1.1",
        "karma-jasmine": "~1.0.2",
        "karma-mocha-reporter": "^2.1.0",
        "karma-sourcemap-loader": "~0.3.7",
        "karma-webpack": "~1.8.0",
        "minx": "r-park/minx.git",
        "morgan": "~1.7.0",
        "node-sass": "~3.10.0",
        "node-uuid": "~1.4.7",
        "nodemon": "~1.10.2",
        "npm-run-all": "~3.1.0",
        "raw-loader": "~0.5.1",
        "sass-loader": "~4.0.1",
        "style-loader": "~0.13.1",
        "semantic-release": "~4.3.5",
        "source-map-loader": "~0.1.5",
        "systemjs": "~0.19.39",
        "systemjs-builder": "~0.15.32",
        "ts-helpers": "~1.1.1",
        "tslint": "~3.15.1",
        "tslint-loader": "2.1.5",
        "typescript": "~2.0.3",
        "url-loader": "~0.5.7",
        "webpack": "2.1.0-beta.25",
        "webpack-dev-server": "2.1.0-beta.4",
        "webpack-shell-plugin": "~0.4.3",
        "winston": "~2.2.0"
    },
    "keywords": [
        "CBP",
        "CF",
        "Common",
        "Frameworks",
        "Team",
        "UI",
        "library",
        "ANGULAR2"
    ]
}
